<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <Button
            android:id="@+id/buttonVehicleType"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Vehicle Type"

            />

        <Button
            android:id="@+id/buttonTypeOfLotTop"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Type of Lot" />

        <Button
            android:id="@+id/buttonCostTop"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Cost" />

        <Button
            android:id="@+id/buttonProximityTop"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Proximity to Current Location" />

        <Button
            android:id="@+id/buttonMoped"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Moped" />

        <Button
            android:id="@+id/buttonCar"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Car" />

        <Button
            android:id="@+id/buttonMotorcycle"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="MotorCycle" />

        <Button
            android:id="@+id/buttonLotType"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Type of Lot" />

        <Button
            android:id="@+id/buttonGarage"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Garage" />

        <Button
            android:id="@+id/buttonSurfaceLot"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Surface Lot" />

        <Button
            android:id="@+id/buttonStreet"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Street" />

        <Button
            android:id="@+id/buttonCost"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Cost" />

        <Button
            android:id="@+id/buttonLowCost"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="$0-$1" />

        <Button
            android:id="@+id/buttonCostMedium"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="$1-$2" />

        <Button
            android:id="@+id/buttonCostHigh"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="More than $2" />

        <Button
            android:id="@+id/buttonProximity"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Proximity to Current Location" />

        <Button
            android:id="@+id/buttonDestinationSmall"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="0 - 0.5 miles" />

        <Button
            android:id="@+id/buttonDestinationMedium"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="0.5 - 1 mile" />

        <Button
            android:id="@+id/buttonDestinationHigh"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="More than 1 miles" />

        <Button
            android:id="@+id/buttonBack"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="Back" />

        <fragment
            android:id="@+id/fragment_map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"/>

        <Button
            android:id="@+id/buttonResetFilters"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Reset Filters" />

        <Button
            android:id="@+id/buttonListview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="List View" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
